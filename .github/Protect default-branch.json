{
  "id": 8515150,
  "name": "Protect default-branch",
  "target": "branch",
  "source_type": "Repository",
  "source": "EnderHostingHQ/template",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "exclude": [],
      "include": [
        "~DEFAULT_BRANCH"
      ]
    }
  },
  "rules": [
    {
      "type": "deletion"
    },
    {
      "type": "non_fast_forward"
    },
    {
      "type": "copilot_code_review",
      "parameters": {
        "review_on_push": true,
        "review_draft_pull_requests": true
      }
    }
  ],
  "bypass_actors": []
}
